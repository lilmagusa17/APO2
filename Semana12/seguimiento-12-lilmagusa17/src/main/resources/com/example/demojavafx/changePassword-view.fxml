<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TableColumn?>

<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.example.demojavafx.control.ChangePasswordViewController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>



    <Label text="CAJA FUERTE" alignment="CENTER" style="-fx-font-weight: bold"></Label>
    <Label text="Digite la combinación actual" alignment="CENTER"></Label>

    <HBox spacing="10.0">
        <TextField fx:id="p1" prefHeight="50.0" prefWidth="100.0" />
        <TextField fx:id="p2" prefHeight="50.0" prefWidth="100.0"/>
        <TextField fx:id="p3" prefHeight="50.0" prefWidth="100.0"/>
        <TextField fx:id="p4" prefHeight="50.0" prefWidth="100.0"/>
        <TextField fx:id="p5" prefHeight="50.0" prefWidth="100.0"/>
        <TextField fx:id="p6" prefHeight="50.0" prefWidth="100.0"/>

    </HBox>
    <Label text="Digite la NUEVA combinación"></Label>

    <HBox spacing="10.0">
        <TextField fx:id="c1" prefHeight="50.0" prefWidth="100.0" />
        <TextField fx:id="c2" prefHeight="50.0" prefWidth="100.0"/>
        <TextField fx:id="c3" prefHeight="50.0" prefWidth="100.0"/>
        <TextField fx:id="c4" prefHeight="50.0" prefWidth="100.0"/>
        <TextField fx:id="c5" prefHeight="50.0" prefWidth="100.0"/>
        <TextField fx:id="c6" prefHeight="50.0" prefWidth="100.0"/>

    </HBox>

    <HBox alignment="CENTER">
        <Button fx:id="btnCambiar" text="CAMBIAR CLAVE" onAction="#cambiarContra"/>
        <Button fx:id="btnClose" text="CERRAR CAJA" onAction="#cerrarCaja"/>

    </HBox>



</VBox>
