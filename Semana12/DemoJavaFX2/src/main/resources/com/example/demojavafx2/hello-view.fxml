<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TableColumn?>
<VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.example.demojavafx2.control.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>

    <TableView fx:id="studentsTableView">
        <columns>
            <TableColumn fx:id="nameTC" text="Name"></TableColumn>
            <TableColumn fx:id="idTC" text="Id"></TableColumn>
            <TableColumn fx:id="ageTC" text="Age"></TableColumn>
        </columns>
    </TableView>

    <Button text="Agregar Estudiante" onAction="#onAddStudent"/>


</VBox>
